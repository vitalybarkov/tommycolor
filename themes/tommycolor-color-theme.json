{
	"name": "tommycolor",
	"colors": {
		"editor.background": "#24282c",
		"editor.foreground": "#eeffff",
		"editor.findMatchBorder": "#ffc72dc9",
		"editor.findMatchHighlightBorder": "#ffc72dc9",
	},
	"tokenColors": [
		{
			"name": "grays",
			"scope": [
				"", // base color for all texts
				"punctuation.definition.string",
				"keyword.type.go",
				"comment",
				"punctuation",
				"meta.brace",
				"storage.modifier",
				"keyword.operator.class.php",
				"keyword.other.rust",
				"support.type.property-name.media.css"
			],
			"settings": {
				"foreground": "#6B7985"
			}
		},
		{
			"name": "bright_grays",
			"scope": [
				"entity.other.attribute-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#bec9d0"
			}
		},
		{
			"name": "whites",
			"scope": [
				"variable",
				"entity.name.type.delegate",
				"entity.name.variable",
				"constant.other",
				"meta.try.body",
				"source.go",
				"source.python",
				"meta.object-literal.key.js",
				"support.type.property-name.json",
				"meta.paragraph",
				"meta.member",
				"source.ignore",
				"source.sql",
				"source.dart",
				"entity.other.attribute-name.class",
				"entity.other.attribute-name.id"
			],
			"settings": {
				"foreground": "#ccd5da"
			}
		},
		{
			"name": "bright_whites",
			"scope": [
				"text.html.derivative",
				"punctuation.definition.entity.html",
				"text.html"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "browns",
			"scope": [
				"constant", // all data
				"string",
				"support.constant",
				"keyword.other.unit",
				"entity.name.type.namespace",
				"storage.modifier.import",
				"entity.name.module",
				"entity.name.package",
				"punctuation.definition.entity.html",
				"constant.other.color.rgb-value.hex.css"
			],
			"settings": {
				"foreground": "#9D9E86"
			}
		},
		{
			"name": "cyans",
			"scope": [
				"entity.name.class",
				"entity.name.type",
				"storage.type",
				"keyword.type",
				"keyword.struct",
				"support.class.dart",
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#37D7B3"
			}
		},
		{
			"name": "blues",
			"scope": [
				"entity.name.function",
				"entity.name.method",
				"keyword",
				"meta.function",
				"support.function.go",
				"storage.type.function.arrow",
				"support.function",
				"entity.other.attribute-name.pseudo-class.css"
			],
			"settings": {
				"foreground": "#3F8BE2"
			}
		},
		{
			"name": "violets",
			"scope": [
				"storage.type.enum",
				"storage.type.struct",
				"storage.type.delegate",
				"storage.type.record",
				"storage.type.class",
				"storage.type.interface",
				"storage.type.namespace",
				"keyword.operator.expression.new",
				"keyword.type.go",
				"keyword.struct.go",
				"keyword.interface.go",
				"keyword.function",
				"keyword.control",
				"storage.type.function",
				"keyword.other.new",
				"keyword.other.fn.rust",
				"storage.type.rust",
				"keyword.declaration.dart"
			],
			"settings": {
				"foreground": "#AD8FFB"
			}
		}
	]
}
